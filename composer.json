{
  "name": "pantheon-upstreams/drupal-composer-managed",
  "description": "Install Drupal 9 with Composer on Pantheon.",
  "type": "project",
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "path",
      "url": "upstream-configuration"
    },
    {
      "type": "package",
      "package": {
        "name": "enyo/dropzone",
        "version": "5.7.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/enyo/dropzone/archive/v5.7.2.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "pantheon-upstreams/upstream-configuration": "dev-main",
    "composer/installers": "^1.9",
    "drupal/core-composer-scaffold": "10.1.0",
    "drupal/core-recommended": "10.1.0",
    "pantheon-systems/drupal-integrations": "^10",
    "cweagans/composer-patches": "^1.7",
    "drush/drush": "^11 || ^12",
    "drupal/admin_toolbar": "^3.4",
    "drupal/config_devel": "^1.9",
    "drupal/config_direct_save": "^3.0",
    "drupal/config_filter": "^2.4",
    "drupal/crop": "^2.3",
    "drupal/devel": "^5.1",
    "drupal/devel_kint_extras": "^1.1",
    "drupal/devel_php": "^1.5",
    "drupal/diff": "^1.1",
    "drupal/focal_point": "^2.0",
    "drupal/google_tag": "^2.0",
    "drupal/imagick": "^1.6",
    "drupal/key": "^1.17",
    "drupal/key_auth": "^2.1",
    "drupal/key_value_field": "^1.3",
    "drupal/pantheon_advanced_page_cache": "^2.1",
    "drupal/paragraphs": "^1.15",
    "drupal/pathauto": "^1.11",
    "drupal/redirect": "^1.8",
    "drupal/redis": "^1.7",
    "drupal/search_api": "^1.29",
    "drupal/search_api_pantheon": "^8.1",
    "drupal/search_api_solr": "^4.2",
    "drupal/simple_block": "^1.4",
    "drupal/token": "^1.12",
    "drupal/token_filter": "^2.0",
    "drupal/viewsreference": "^1.8",
    "pingevt/timing_monitor": "^1.0@alpha",
    "drupal/gin": "^3.0@RC",
    "drupal/gin_login": "^2.0",
    "drupal/layout_paragraphs": "^2.0",
    "drupal/metatag": "^2.0",
    "drupal/entity_usage": "^2.0@beta",
    "drupal/highlightjs_input_filter": "^1.0",
    "drupal/recreate_block_content": "^3.0",
    "drupal/menu_item_extras": "^3.0",
    "drupal/editoria11y": "^2.0",
    "drupal/simple_sitemap": "^4.1",
    "drupal/node_revision_delete": "^2.0@alpha",
    "drupal/dropzonejs": "^2.8",
    "enyo/dropzone": "^5.7",
    "drupal/entity_browser": "^2.9",
    "drupal/media_bulk_upload": "^3.0",
    "drupal/datalayer": "^2.0@RC",
    "drupal/better_exposed_filters": "^6.0",
    "drupal/views_ajax_history": "^1.7",
    "drupal/selective_better_exposed_filters": "^3.0@beta",
    "drupal/core-project-message": "10.1.0",
    "pingevt/vis_reg_result": "1.0.0-alpha5",
    "pingevt/external_site_monitor": "^1.0@RC",
    "drupal/gin_toolbar": "^1.0@RC"
  },
  "require-dev": {
    "drupal/core-dev": "^10"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "./web"
      },
      "allowed-packages": [
        "pantheon-systems/drupal-integrations"
      ],
      "file-mapping": {
        "[project-root]/.editorconfig": false,
        "[project-root]/pantheon.upstream.yml": false,
        "[project-root]/.gitattributes": false
      }
    },
    "installer-types": [
      "custom-drupal-module"
    ],
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/modules/bluecadet/{$name}": [
        "type:custom-drupal-module"
      ],
      "web/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],
      "web/private/scripts/quicksilver/{$name}/": [
        "type:quicksilver-script"
      ]
    },
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "enable-patching": true,
    "patches": {
      "drupal/save_edit": {
        "D10 compatability https://www.drupal.org/project/save_edit/issues/3289467": "https://www.drupal.org/files/issues/2022-06-16/save_edit.1.x-dev.rector.patch"
      },
      "drupal/core": {
        "CKEditor5 modal thingy https://www.drupal.org/project/drupal/issues/3328425": "https://www.drupal.org/files/issues/2022-12-27/3328425.patch"
      }
    }
  },
  "autoload": {
    "classmap": [
      "upstream-configuration/scripts/ComposerScripts.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "rm -f web/.eslintrc.json",
      "rm -f web/core/.eslintrc.json"
    ],
    "pre-update-cmd": [
      "DrupalComposerManaged\\ComposerScripts::preUpdate"
    ],
    "post-update-cmd": [
      "DrupalComposerManaged\\ComposerScripts::postUpdate",
      "rm -f web/.eslintrc.json",
      "rm -f web/core/.eslintrc.json"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": false,
    "platform": {
      "php": "8.2.0"
    },
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "oomphinc/composer-installers-extender": true,
      "drupal/core-project-message": true
    }
  }
}
